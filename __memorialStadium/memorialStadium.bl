(***--- WHITE PERIMETER OF FIELD ---***)
fun outOfBounds(x,y,z) = 
    (
        put (yard*123,yard,yard*56) WHITE (x,y,z)
    );


(***--- THE RECTANGLES ON SIDELINES ON BOTH SIDES OF FIELD ---***)
fun homeCoachesBox(x,y,z) = 
    (
      (*east side*)
        put (yard   ,yard,yard*2) WHITE (x   ,y,z+3);
        put (yard   ,yard,yard*2) WHITE (x+50,y,z+3);
        put (yard*51,yard,yard  ) WHITE (x   ,y,z+2)
    );

fun awayCoachesBox(x,y,z) = 
    (
      (*west side*)
        put (yard   ,yard,yard*2) WHITE (x   ,y,z  );
        put (yard   ,yard,yard*2) WHITE (x+50,y,z  );
        put (yard*51,yard,yard  ) WHITE (x   ,y,z+2)
    );


(***--- THE SOLID WHITE LINE MARKERS EVERY 10 YARDS ---***)
fun yardMarkers(xStart,yStart,zStart) =
    let
        fun tenYardLines x = (x - xStart) mod 10 = 0;

        fun paintYardLines(x,y,z) =
            if tenYardLines x then WHITE
            else GREEN;
    in
        traverseWithin (xStart,yStart,zStart) (xStart+xFieldLength,yStart,zStart+zFieldWidth) paintYardLines
    end;


(***--- THE TWO HASH MARKS THAT RUN DOWN THE MIDDLE OF FIELD ---***)
fun hashMarks(xStart,yStart,zStart) = 
    let
        fun hashes (x,y,z) = put (yard,yard,yard*2) WHITE (x,y,z);

        fun multipleOfTen v = v mod 10 = 0

        fun makeHashMarks (x,y,z) = 
            let
                val x1 = x - xStart
            in
                if multipleOfTen x1 orelse multipleOfTen (x1 - 4) then hashes (x,y,z) else ()
            end;
    in
        applyWithin (xStart,yStart,zStart) (xStart + yard*94,yStart,zStart) makeHashMarks
    end;


(***--- THE TWO HASHES IN MIDDLE OF FIELD NEAR GOAL LINE FOR EXTRA POINTS ---***)
fun extraPointHash(xStart,yStart,zStart) = 
    let
        fun twoPointHash (x,y,z) = put (yard,yard,yard*2) WHITE (x,y,z);
        
        fun placeTwoPointHash v = v mod 94 = 0

        fun makeTwoPointHash (x,y,z) =
            let 
                val x1 = x - xStart
            in
                if placeTwoPointHash x1 orelse placeTwoPointHash (x1 - 94) then twoPointHash (x,y,z) else ()  
            end;
    in
        applyWithin (xStart,yStart,zStart) (xStart + yard*94,yStart,zStart) makeTwoPointHash
    end;


(***--- ALTERNATES THE FIELD BETWEEN GREEN AND DARK GREEN ---***)
fun stripedField(xStart,yStart,zStart) = 
    let
        val xStripeSize = yard *  9;
        val zStripeSize = yard * 52;

        fun stripe (x,y,z) = put (xStripeSize,yard,zStripeSize) DARKGREEN (x,y,z);

        fun makeStripes (x,y,z) =
            let
                fun multipleOfTwenty v = v mod 20 = 0;

                val x1 = x - xStart;
            in
                if multipleOfTwenty x1 then stripe (x,y,z) else ()
            end;
    in
        applyWithin (xStart,yStart,zStart) (xStart + yard * 100,yStart,zStart) makeStripes
    end;


